{
	"name": "janten-master",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"test:unit": "vue-cli-service test:unit",
		"lint": "vue-cli-service lint",
		"test": "mocha --require babel-core/register"
	},
	"dependencies": {
		"babel-core": "^6.26.3",
		"core-js": "^3.6.5",
		"vue": "^2.6.11",
		"vue-select-image": "^1.9.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-plugin-unit-mocha": "~4.5.15",
		"@vue/cli-service": "~4.5.0",
		"@vue/test-utils": "^1.0.3",
		"babel-eslint": "^10.1.0",
		"chai": "^4.3.6",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"fetch-mock": "^9.11.0",
		"jest": "^27.5.1",
		"jest-html-reporters": "^3.0.5",
		"mocha": "^9.2.1",
		"node-fetch": "^3.2.0",
		"sinon": "^13.0.1",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {},
		"overrides": [
			{
				"files": [
					"**/__tests__/*.{j,t}s?(x)",
					"**/tests/unit/**/*.spec.{j,t}s?(x)"
				],
				"env": {
					"mocha": true
				}
			}
		]
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
